    "require": {
        "illuminate/support": ">=v8.83.0",
        "php": ">=8.1",
        "ext-curl": "*"
    },

    "require-dev": {
        "phpunit/phpunit": ">=9.5.21"
    },
    "autoload": {
        "psr-4": {
            "DevNullIr\\LaravelFreeHost\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
